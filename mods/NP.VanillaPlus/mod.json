{
  "Name" : "Nano.VanillaPlus",
  "Description": "Overrides just barely enough of northstar for some basic client-side mod support on vanilla servers.\nAlso enables the DiscordRPC plugin.",
  "LoadPriority": 1,
  "Version": "1.3.3",
  "ConVars": [
    {
			"Name": "ns_is_modded_server",
			"DefaultValue": "0"
		}
  ],
  "Scripts": [
   {
      "Path": "_custom_codecallbacks.gnut",
      "RunOn": "SERVER"
   }, 
   {
  	  "Path": "vanilla_plus_ui_init.gnut",
	    "RunOn": "UI",
      "UICallback": {
        "Before": "VanillaPlusUI_Init",
        "After": "VanillaPlusUI_LateInit"
	    }
   }
  ]
}
